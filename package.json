{
  "name": "bravoecho",
  "version": "0.0.1",
  "description": "Bravo Echo Web Development",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run dev",
    "server": "nodemon server.js",
    "start:prod": "node server.js",
    "dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "npm start  --prefix client",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianevans99/BravoEcho.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brianevans99/BravoEcho/issues"
  },
  "homepage": "https://github.com/brianevans99/BravoEcho#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.3"
  }
}
